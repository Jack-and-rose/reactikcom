{
  "presets": ["es2015", "react", "stage-0"],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    }
  },
  "plugins": [
      [
          "import", {
              "libraryName": "antd",
              "libraryDirectory": "es",
              "style": "css"
          }
      ],
      [
          "transform-runtime",
          {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
          }
      ],
      ["transform-decorators-legacy"]
  ]
}